    "dotenv": "^16.3.1",
    "fast-csv": "^5.0.5",
    "ioredis": "^5.3.2",
    "joi": "^18.0.2",
    "nestjs-i18n": "^10.6.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-oauth2": "^1.8.0",
    "passport-twitter": "^1.0.4",
